.divider1{
  height: 1px;
  margin: 20rpx 0 30rpx 0;
  background-color: var(--divider);
}

.divider2{
  height: 1px;
  margin: 20rpx 0 20rpx 60rpx;
  background-color: var(--divider);
}

.outer{
  margin:40rpx 30rpx 40rpx 30rpx;
  width:690rpx;
  border-radius:10rpx;
  font-size: 30rpx;
  background-color: var(--bg-2);
  box-shadow: 0px 0px 10px rgba(0,0,0,0.16);
}

.function_container{
  margin-top: 20rpx;
  text-align:center;
  background-color:var(--model-bg-gray);
  border-radius: 10px;
  display: flex;
  width: 630rpx;
}

.function_text_container{
  padding-left: 20rpx;
  padding-right:20rpx;
  padding-top:10rpx;
  padding-bottom:10rpx;
  display: flex;
  flex-direction: column;
  position: relative;
  top: 50%;
  transform: translate(0, -50%);
}

.function_text_title{
  text-align: left;
  font-size: 28rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.function_text_detail{
  text-align: left;
  font-size: 28rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.function_text_detail_quote{
  text-align: left;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.function_bilibili_icon{
  width:80rpx;
  height:40rpx;
  position: absolute;
  bottom: 15rpx;
  right: 15rpx;
}

.function_netease_icon{
  width:40rpx;
  height:40rpx;
  position: absolute;
  bottom: 15rpx;
  right: 15rpx;
}

.function_bilibili_image{
  height:150rpx;
  width:230rpx;
  border-radius: 20rpx
}

.function_netease_image{
  height:150rpx;
  width:150rpx;
  border-radius: 20rpx
}

.function_bilibili_right{
  width: 400rpx;
  height: 150rpx;
  position: relative;
}

.function_netease_right{
  width: 480rpx;
  height: 150rpx;
  position: relative;
}

.function_quote_right{
  width: 480rpx;
  height: 150rpx;
  position: relative;
}

.function_vote_title{
  margin-left:30rpx;
  margin-right:30rpx;
  margin-top:20rpx;
  text-align: left;
  font-size: 32rpx;
  font-weight: bold;
}

.function_vote_subtitle{
  margin-left:30rpx;
  margin-right:30rpx;
  margin-top: 20rpx;
  text-align: left;
  font-size: 26rpx;
}

.function_vote_option_list{
  background-color: var(--bg-1);
  height: 70rpx;
  margin: 20rpx 20rpx 10rpx 20rpx;
  border-radius: 20rpx;
  position: relative;
}

.function_vote_option_list_selected{
  background-color: var(--bg-1);
  height: 70rpx;
  margin: 20rpx 20rpx 10rpx 20rpx;
  border-radius: 20rpx;
  position: relative;
}

.function_vote_option_lower{
  height:100%;
  width: 100%;
  background-color: var(--bg-1);
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  position: relative;
  overflow: hidden;
}

.function_vote_option_upper{
  height:100%;
  width: 0;
  background: var(--primary);
  border-radius: 20rpx;
  position: absolute;
  top:0;
  left:0;
  overflow: hidden;
}

.function_vote_option_upper_voted{
  height:100%;
  width: 0;
  background: var(--secondary);
  border-radius: 20rpx;
  position: absolute;
  top:0;
  left:0;
  overflow: hidden;
}

.function_vote_option_upper_inner{
  width: 560rpx;
  height: 60rpx;
  position: relative;
}

.function_vote_option_lower_text_left{
  color:var(--primary);
  font-size: 30rpx;
  position: absolute;
  text-align: left;
  left:30rpx;
  right:100rpx;
  top: 15rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis
}

.function_vote_option_lower_text_right{
  color:var(--primary);
  font-size: 30rpx;
  position: absolute;
  right:30rpx;
  top: 15rpx;
  white-space:nowrap;
}

.function_vote_option_lower_text_left_voted{
  color:var(--secondary);
  font-size: 30rpx;
  position: absolute;
  text-align: left;
  left:30rpx;
  right:100rpx;
  top: 15rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis
}

.function_vote_option_lower_text_right_voted{
  color:var(--secondary);
  font-size: 30rpx;
  position: absolute;
  right:30rpx;
  top: 15rpx;
  white-space:nowrap;
}

.function_vote_option_upper_text_left{
  color:var(--bg-1);
  font-size: 30rpx;
  position: absolute;
  left:30rpx;
  top: 15rpx;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis
}

.function_vote_option_upper_text_right{
  color:var(--bg-1);
  font-size: 30rpx;
  position: absolute;
  right:30rpx;
  top: 15rpx;
  white-space:nowrap;
}

.org-box{
  height:40rpx;
  margin-top: 85rpx;
  margin-bottom: 20rpx;
  margin-right: 0rpx;
  position:relative; 
  border-radius: 25rpx;
  width: auto;
}
.org-box-cor{
  width:0; 
  height:0; 
  font-size:0; 
  border-width:15rpx; 
  border-style:solid; 
  border-color:transparent transparent var(--inputouter) transparent; 
  overflow:hidden; 
  position:absolute; 
  right:40rpx; 
  top:-25rpx;
}
.org-box-text{
  padding-left:20rpx;
  padding-right:20rpx;
  position: absolute;
  top: 50%;
  transform: translate(0, -50%);
  right: 10rpx;
  font-size: 20rpx;
  font-weight: bold;
  text-align: right;
  margin-right: 10rpx;
  width: auto;
  background: var(--inputouter);
  height:40rpx;
  border-radius: 25rpx;
}
.org-box-inner{
  position: relative;
  top: 50%;
  transform: translate(0, -50%);
}