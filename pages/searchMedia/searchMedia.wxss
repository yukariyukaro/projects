page {
  height: 100%;
  box-sizing: border-box;
}
.outer {
  width: 100vw
}
.search-outer {
  display: flex;
  padding-top: 20vw;
  width: 90vw;
  margin: auto
}
.search-input-outer {
  height: 10vw;
  width: 60vw;
  border-radius: 5vw;
  background-color: var(--bg-2);
  box-shadow: var(--shadow-3);
  flex: 1;
  align-self: center;
  display: flex;
  font-size: 4vw;
  line-height: 6vw;
}
.search-prefix {
  margin-left: 3vw;
  justify-content: center;
  display: flex;
  flex-direction: column;
}
.prefix-media-icon {
  width: 4vw;
  height: 4vw;
}
.search-input {
  align-self: center;
  margin: 0 3vw;
  width: 100%;
}
.search-icon-outer {
  align-self: center;
  width: 10vw;
  height: 10vw;
  border-radius: 5vw;
  background-color: var(--bg-2);
  box-shadow: var(--shadow-3);
  margin-left: 5vw;
  display: flex
}
.search-icon {
  width: 5vw;
  height: 5vw;
  margin: auto
}
.delete-icon-outer {
  align-self: center;
  margin-right: 2vw;  
  display: flex;
}
.close-icon {
  width: 5vw;
  height: 5vw;
}
.result-outer {
  font-size: 4vw;
  color: var(--on-bg-2);
  padding-top: 10vw;
  margin: auto
}
.result {
  margin: auto;
  width: 85%;
}